Cell[TextData[{
 "(To keep the picture contained, I scaled the red vector down by a factor of \
2). Now, we see the whole story. Since the two curves share a common tangent \
line at the point (1,2), the two gradients need not be the same vector, but \
they must be parallel. So, one will be a scalar multiple of the other. In our \
case, since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[Del]", 
      RowBox[{"C", "(", 
       RowBox[{"1", ",", "2"}], ")"}]}], "=", 
     RowBox[{"<", 
      RowBox[{"-", "12"}]}]}], ",", 
    RowBox[{"12", ">"}]}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[Del]", 
      RowBox[{"f", "(", 
       RowBox[{"x", ",", "y"}], ")"}]}], "=", 
     RowBox[{"<", "1"}]}], ",", 
    RowBox[{
     RowBox[{"-", "1"}], ">"}]}], TraditionalForm]]],
 ", we see ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Del]", 
     RowBox[{"C", "(", 
      RowBox[{"1", ",", "2"}], ")"}]}], "=", 
    RowBox[{
     RowBox[{"-", "12"}], 
     RowBox[{"\[Del]", 
      RowBox[{"f", "(", 
       RowBox[{"1", ",", "2"}], ")"}]}]}]}], TraditionalForm]]],
 ". The number ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", "12"}], TraditionalForm]]],
 " in this context is called a LaGrange multiplier. "
}], "Text",
 CellChangeTimes->{{3.6375932338021946`*^9, 3.6375935920432243`*^9}, 
   3.6377468954409094`*^9}],

Cell[TextData[{
 StyleBox["The logic behind the process:\n\nLet\[CloseCurlyQuote]s suppose \
that the maximum (or minimum) value for the function ",
  FontSize->18],
 StyleBox["f",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" on the curve ",
  FontSize->18],
 StyleBox["C",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" is the number ",
  FontSize->18],
 StyleBox["k",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" and that this occurs at the point ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["x", "0"], ",", 
     SubscriptBox["y", "0"]}], ")"}], TraditionalForm]],
  FontSize->18],
 StyleBox[".",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" Lastly, assume that the curve ",
  FontSize->18],
 StyleBox["C",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" is described by an equation of the form ",
  FontSize->18],
 StyleBox["C(x,y)=c",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[".\n\n1. Then, in most cases, the level set for ",
  FontSize->18],
 StyleBox["f",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" is tangent to the curve ",
  FontSize->18],
 StyleBox["C",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" at the point ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["x", "0"], ",", 
     SubscriptBox["y", "0"]}], ")"}], TraditionalForm]],
  FontSize->18],
 StyleBox[".\n2. So, at this point, \[Del]",
  FontSize->18],
 StyleBox["f",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" is parallel to \[Del]",
  FontSize->18],
 StyleBox["C",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" .\n3. This means that there is a real number \[Lambda] so that \
\[Del]",
  FontSize->18],
 StyleBox["f",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[" =\[Lambda] \[Del]",
  FontSize->18],
 StyleBox["C",
  FontSize->18,
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["x", "0"], ",", 
     SubscriptBox["y", "0"]}], ")"}], TraditionalForm]],
  FontSize->18],
 ". ",
 StyleBox["That is to say, ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     RowBox[{"<", 
      RowBox[{
       SubscriptBox["f", "x"], "(", 
       RowBox[{
        SubscriptBox["x", "0"], ",", 
        SubscriptBox["y", "0"]}], ")"}]}], ",", 
     RowBox[{
      RowBox[{
       SubscriptBox["f", "y"], "(", 
       RowBox[{
        SubscriptBox["x", "0"], ",", 
        SubscriptBox["y", "0"]}], ")"}], ">=", "<", 
      RowBox[{
       SubscriptBox[
        StyleBox[
         RowBox[{
          StyleBox["\[Lambda]",
           FontSize->18], "C"}]], "x"], "(", 
       RowBox[{
        SubscriptBox["x", "0"], ",", 
        SubscriptBox["y", "0"]}], ")"}]}], ",", 
     RowBox[{
      RowBox[{
       SubscriptBox[
        StyleBox[
         RowBox[{
          StyleBox["\[Lambda]",
           FontSize->18], "C"}]], "y"], "(", 
       RowBox[{
        SubscriptBox["x", "0"], ",", 
        SubscriptBox["y", "0"]}], ")"}], ">"}]}],
    FontSize->18], TraditionalForm]]],
 " ",
 StyleBox[".\n",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox["4. This means, the following equations are all satisfied:\n\t\ti. ",
  FontSize->18],
 StyleBox["C",
  FontSize->18,
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["x", "0"], ",", 
     SubscriptBox["y", "0"]}], ")"}], TraditionalForm]],
  FontSize->18],
 StyleBox["=c",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox["\n\t\tii. ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["f", "x"], "(", 
     RowBox[{
      SubscriptBox["x", "0"], ",", 
      SubscriptBox["y", "0"]}], ")"}], "=", 
    RowBox[{
     SubscriptBox["\[Lambda]C", "x"], "(", 
     RowBox[{
      SubscriptBox["x", "0"], ",", 
      SubscriptBox["y", "0"]}], ")"}]}], TraditionalForm]],
  FontSize->18],
 StyleBox["\n\t\tiii. ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["f", "y"], "(", 
     RowBox[{
      SubscriptBox["x", "0"], ",", 
      SubscriptBox["y", "0"]}], ")"}], "=", 
    RowBox[{
     SubscriptBox["\[Lambda]C", "y"], "(", 
     RowBox[{
      SubscriptBox["x", "0"], ",", 
      SubscriptBox["y", "0"]}], ")"}]}], TraditionalForm]],
  FontSize->18],
 StyleBox["\n\t\tiv. ",
  FontSize->18],
 StyleBox["f(",
  FontSize->18,
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "0"], ",", 
    SubscriptBox["y", "0"]}], TraditionalForm]],
  FontSize->18],
 StyleBox[")=k",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox["\n\t\t",
  FontSize->18],
 " "
}], "Text",
 CellChangeTimes->{{3.637599468302786*^9, 3.637599476375248*^9}, {
  3.637599532571462*^9, 3.637599564488288*^9}, {3.637599858059079*^9, 
  3.6376001322687626`*^9}, {3.6376001950373535`*^9, 3.6376001960944138`*^9}, {
  3.6376002454692373`*^9, 3.637600916372611*^9}, {3.6377469705912075`*^9, 
  3.637747005928229*^9}, {3.649325149696662*^9, 3.649325376929659*^9}, {
  3.6493254409933233`*^9, 3.6493254739622087`*^9}, {3.65668065083335*^9, 
  3.65668066564025*^9}}],

Cell[TextData[{
 "If you look at these four equations, you will notice that there are four \
unknowns, namely, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "0"], ",", " ", 
    SubscriptBox["y", "0"], ",", " ", "\[Lambda]"}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]]],
 ". Moreover, ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]]],
 " only appears in the last one. So, the process is to solve the first three \
equations for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "0"], ",", " ", 
    SubscriptBox["y", "0"]}], TraditionalForm]]],
 " and \[Lambda]. We might have several solutions for the pair ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["x", "0"], ",", 
     SubscriptBox["y", "0"]}], ")"}], TraditionalForm]]],
 ". However, it is true that any extrema will happen at such a point. So, if \
we get several answers, we just plug them all in to find the max and the min.\
\n\nIf you are a careful reader, you will have noticed the word \
\[OpenCurlyDoubleQuote]most\[CloseCurlyDoubleQuote] that appears in the first \
item doesn\[CloseCurlyQuote]t appear in the above paragraph. That is because \
the only exceptions to the tangency critereon happen when the gradient of ",
 StyleBox["f",
  FontSlant->"Italic"],
 " is ",
 StyleBox["<0,0>",
  FontSlant->"Italic"],
 ". In such cases, solving the system of equations will yield a soultion with \
\[Lambda]=0."
}], "Text",
 CellChangeTimes->{{3.637600951806638*^9, 3.637601416819235*^9}, {
  3.637609611434434*^9, 3.6376096246141877`*^9}, {3.6377470204140573`*^9, 
  3.637747028433516*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["An example:", "Section",
 CellChangeTimes->{{3.637609666300572*^9, 3.637609672368919*^9}}],

Cell["\<\
Now, let\[CloseCurlyQuote]s work through the problem we studied in the last \
technology assignment where we had been looking at the graph. Recall, what \
the question is:
\
\>", "Text",
 CellChangeTimes->{{3.6376096977093687`*^9, 3.637609724051875*^9}, {
   3.6376097988351526`*^9, 3.637609813703003*^9}, 3.637747057729192*^9, {
   3.6617718390453095`*^9, 3.66177186821931*^9}}],

Cell[TextData[{
 "                                 Find the extrema of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{"x", ",", "y"}], ")"}], "=", 
    RowBox[{
     RowBox[{"3", "x"}], "-", 
     RowBox[{"4", "y"}]}]}], TraditionalForm]]],
 " on the circle ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", "2"], "+", 
     SuperscriptBox["y", "2"]}], "=", "25"}], TraditionalForm]]],
 "."
}], "Subsubsection",
 CellChangeTimes->{{3.637609826522736*^9, 3.6376098939495926`*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "\nHere, our constraint function is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", 
     RowBox[{"x", ",", "y"}], ")"}], "=", 
    RowBox[{
     SuperscriptBox["x", "2"], "+", 
     SuperscriptBox["y", "2"]}]}], TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"c", "=", "25"}], TraditionalForm]]],
 " and the function that we want the extrema of is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{"x", ",", "y"}], ")"}], "=", 
    RowBox[{
     RowBox[{"3", "x"}], "-", 
     RowBox[{"4", "y"}]}]}], TraditionalForm]]],
 ".\nWe need to solve the following system of equations for ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox["\[Lambda]", TraditionalForm]]],
 ".\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{"C", "(", 
      RowBox[{"x", ",", "y"}], ")"}], "=", "c"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{
      SubscriptBox["f", "x"], "(", 
      RowBox[{"x", ",", "y"}], ")"}], "=", 
     RowBox[{
      SubscriptBox["\[Lambda]C", "x"], "(", 
      RowBox[{"x", ",", "y"}], ")"}]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{
      SubscriptBox["f", "y"], "(", 
      RowBox[{"x", ",", "y"}], ")"}], "=", 
     RowBox[{
      SubscriptBox["\[Lambda]C", "y"], "(", 
      RowBox[{"x", ",", "y"}], ")"}]}]}], TraditionalForm]]],
 "\n\nIn this case, that is the system of equations:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["x", "2"], "+", 
       SuperscriptBox["y", "2"]}], "=", "25"}], "\[IndentingNewLine]", "\t", 
     RowBox[{"3", "=", 
      RowBox[{"\[Lambda]", "(", 
       RowBox[{"2", "x"}], ")"}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"-", "4"}], "=", 
      RowBox[{"\[Lambda]", "(", 
       RowBox[{"2", "y"}], ")"}]}]}]}], TraditionalForm]]]
}], "Subsubsection",
 CellChangeTimes->{{3.637609921305157*^9, 3.6376103117984924`*^9}, 
   3.6376842102579036`*^9, 3.6377470677677655`*^9}],

Cell["\<\
To solve this system, we could take out a pen and paper (in fact, we should \
be able to do it that way), but hey, we\[CloseCurlyQuote]re sitting at a \
computer.\
\>", "Text",
 CellChangeTimes->{{3.637610320902013*^9, 3.6376103628184104`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"NSolve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["x", "2"], "+", 
      SuperscriptBox["y", "2"]}], "\[Equal]", "25"}], "&&", 
    RowBox[{"3", "\[Equal]", 
     RowBox[{"\[Lambda]", "*", "2", "x"}]}], "&&", 
    RowBox[{
     RowBox[{"-", "4"}], "==", 
     RowBox[{"\[Lambda]", "*", "2", "y"}]}]}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y", ",", "\[Lambda]"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.6376103687007465`*^9, 3.6376104600549717`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "\[Rule]", 
      RowBox[{"-", "3.`"}]}], ",", 
     RowBox[{"y", "\[Rule]", "4.`"}], ",", 
     RowBox[{"\[Lambda]", "\[Rule]", 
      RowBox[{"-", "0.5`"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "\[Rule]", "3.`"}], ",", 
     RowBox[{"y", "\[Rule]", 
      RowBox[{"-", "4.`"}]}], ",", 
     RowBox[{"\[Lambda]", "\[Rule]", "0.5`"}]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{3.637610462933137*^9}]
}, Open  ]],

Cell[TextData[{
 "We see we got two answers: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"x", ",", "y"}], ")"}], "=", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "3"}], ",", "4"}], ")"}]}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"x", ",", "y"}], ")"}], "=", 
    RowBox[{"(", 
     RowBox[{"3", ",", 
      RowBox[{"-", "4"}]}], ")"}]}], TraditionalForm]]],
 ". To find the corresponding values of ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]]],
 ", we could plug these points in. Or, better yet, let\[CloseCurlyQuote]s \
adapt that bit last code to also tell us the outputs (which I\
\[CloseCurlyQuote]ll call ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]]],
 ")."
}], "Text",
 CellChangeTimes->{{3.6376104947609572`*^9, 3.6376106119646606`*^9}, {
  3.63774711365839*^9, 3.6377471148294573`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"NSolve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["x", "2"], "+", 
      SuperscriptBox["y", "2"]}], "\[Equal]", "25"}], "&&", 
    RowBox[{"3", "\[Equal]", 
     RowBox[{"\[Lambda]", "*", "2", "x"}]}], "&&", 
    RowBox[{
     RowBox[{"-", "4"}], "==", 
     RowBox[{"\[Lambda]", "*", "2", "y"}]}], "&&", 
    RowBox[{
     RowBox[{
      RowBox[{"3", "x"}], "-", 
      RowBox[{"4", "y"}]}], "\[Equal]", "k"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y", ",", "\[Lambda]", ",", "k"}], "}"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.637610597936858*^9, 3.637610635075983*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "\[Rule]", 
      RowBox[{"-", "3.`"}]}], ",", 
     RowBox[{"y", "\[Rule]", "4.`"}], ",", 
     RowBox[{"\[Lambda]", "\[Rule]", 
      RowBox[{"-", "0.5`"}]}], ",", 
     RowBox[{"k", "\[Rule]", 
      RowBox[{"-", "25.`"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "\[Rule]", "3.`"}], ",", 
     RowBox[{"y", "\[Rule]", 
      RowBox[{"-", "4.`"}]}], ",", 
     RowBox[{"\[Lambda]", "\[Rule]", "0.5`"}], ",", 
     RowBox[{"k", "\[Rule]", "25.`"}]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{{3.6376106290016356`*^9, 3.6376106359290314`*^9}}]
}, Open  ]],

Cell[TextData[{
 "Now we have our answer: ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]]],
 " achieves its minimum at (-3,4), and its value there is -25; ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]]],
 " achieves its maximum at (3,-4), and its value there is 25."
}], "Text",
 CellChangeTimes->{{3.637610650453862*^9, 3.637610728021299*^9}}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Another example:", "Section",
 CellChangeTimes->{{3.6376108721855445`*^9, 3.6376108768498116`*^9}}],

Cell[TextData[{
 "Here\[CloseCurlyQuote]s another question we answered in the previous \
technology assignment:\n\n  ",
 StyleBox["A question:",
  FontWeight->"Bold"],
 " Use a picture to approximate the extreme values for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{"x", ",", "y"}], ")"}], "=", 
    RowBox[{
     RowBox[{"4", 
      SuperscriptBox["x", "2"]}], "+", 
     RowBox[{"9", 
      SuperscriptBox["y", "2"]}]}]}], TraditionalForm]]],
 " on the constraint curve ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", ":", " ", "xy"}], "=", "4"}], TraditionalForm]]],
 "\n  \n  We can now answer it without a picture and without approximating"
}], "Text",
 CellChangeTimes->{{3.6376108572356896`*^9, 3.6376109437956405`*^9}, {
  3.6617719829093094`*^9, 3.6617720080973096`*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "We need to solve the following system of equations for ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox["\[Lambda]", TraditionalForm]]],
 ".\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{"C", "(", 
      RowBox[{"x", ",", "y"}], ")"}], "=", "c"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{
      SubscriptBox["f", "x"], "(", 
      RowBox[{"x", ",", "y"}], ")"}], "=", 
     RowBox[{
      SubscriptBox["\[Lambda]C", "x"], "(", 
      RowBox[{"x", ",", "y"}], ")"}]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{
      SubscriptBox["f", "y"], "(", 
      RowBox[{"x", ",", "y"}], ")"}], "=", 
     RowBox[{
      SubscriptBox["\[Lambda]C", "y"], "(", 
      RowBox[{"x", ",", "y"}], ")"}]}]}], TraditionalForm]]],
 "\nIn this case:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{"xy", "=", "4"}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"8", "x"}], "=", 
      RowBox[{"\[Lambda]", "(", "y", ")"}]}], "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"18", "y"}], "=", 
      RowBox[{"\[Lambda]", "(", "x", ")"}]}]}]}], TraditionalForm]]],
 "\nHere\[CloseCurlyQuote]s the code that does it:"
}], "Subsubsection",
 CellChangeTimes->{{3.637610973822358*^9, 3.6376110541989555`*^9}, {
  3.637611097716444*^9, 3.6376111437130747`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"NSolve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"x", "*", "y"}], "\[Equal]", "4"}], "&&", 
    RowBox[{
     RowBox[{"8", "x"}], "\[Equal]", 
     RowBox[{"\[Lambda]", "*", "y"}]}], "&&", 
    RowBox[{
     RowBox[{"18", "y"}], "==", 
     RowBox[{"\[Lambda]", "*", "x"}]}], "&&", 
    RowBox[{
     RowBox[{
      RowBox[{"4", 
       SuperscriptBox["x", "2"]}], "+", 
      RowBox[{"9", 
